  "hyprland/window": {
    "format": "{title} - {class}",
    "tooltip": true,
    "max-length": 50,

    "rewrite": {
      " - ": "",
      "(.*) - dev\\.zed\\.Zed": "$1",
      "(.*) - TUI\\.float": "$1",
      "(.*) - Cursor": "$1",
      "(.*) - Google-Chrome": "$1",
      "(.*nvim)...(.*)": "$2",
      "(.*code) - (.*)": "$1",
      "^.*btop( .*|$)": "BTOP$1",
      "^.*unimatrix( .*|$)": "MATRIX $1",
      "(.*) - Spotify": "$1",
      "(.*) - com.mitchellh.ghostty": "$1",
      "(.*) - Alacritty": "$1",
      "(.*Discord)...(.*)": "$2",
      "(.*) - Vesktop": "$1",
      "(.*) - org.gnome.Nautilus": "$1",
      "(.*Steam)...(.*)": "$1",
      "(.*imv)...(.*).. (.*)": "$2",
      "(.*) - com.obsproject.Studio": "$1",
      "(.*) - mpv": "$1",
    },
  },

